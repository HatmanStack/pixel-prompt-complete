# Pixel Prompt v2 Deployment Configuration
# Copy this file to .env.deploy and fill in your values
# .env.deploy is gitignored and will not be committed

# AWS Configuration
AWS_REGION=us-west-2
STACK_NAME=pixel-prompt-v2

# Rate Limiting
GLOBAL_RATE_LIMIT=1000
IP_RATE_LIMIT=100

# ============================================
# PROMPT ENHANCEMENT MODEL (Text/Chat Model)
# ============================================
PROMPT_MODEL_PROVIDER=openai
PROMPT_MODEL_ID=gpt-4o
PROMPT_MODEL_API_KEY=sk-your-openai-key

# ============================================
# 4 FIXED IMAGE GENERATION MODELS
# ============================================

# Flux (Black Forest Labs)
FLUX_ENABLED=true
FLUX_API_KEY=your-bfl-api-key
FLUX_MODEL_ID=flux-pro-1.1

# Recraft v3
RECRAFT_ENABLED=true
RECRAFT_API_KEY=your-recraft-api-key
RECRAFT_MODEL_ID=recraftv3

# Google Gemini
GEMINI_ENABLED=true
GEMINI_API_KEY=your-google-api-key
GEMINI_MODEL_ID=gemini-2.0-flash-exp

# OpenAI gpt-image-1
OPENAI_ENABLED=true
OPENAI_API_KEY=sk-your-openai-key
OPENAI_MODEL_ID=gpt-image-1
